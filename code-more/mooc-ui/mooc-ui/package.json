    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-minify-css": "^1.2.4",
    "gulp-sass": "^5.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "node-sass": "^7.0.1",
    "sass": "^1.45.2",
    "sass-loader": "^5.0.1",
    "vue-loader": "^15.9.8",
    "vue-template-compiler": "^2.6.11",
    "vuepress": "^1.9.5"
  },
  "eslintConfig": {
